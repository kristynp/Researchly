<h1>Log In</h1>
<div>
  <%= form_for :user do |f| %>
    <div class="container center" class="form-group" >
      <div>
      <%= f.label :user_name%>
      <%= f.text_field :user_name, class: "form-control" %>
      </div>
      <div>
      <%= f.label :password %>
      <%= f.password_field :password, class: "form-control" %>
      </div>
      <%= f.submit "Login" %>
    </div>
  <% end %>
</div>