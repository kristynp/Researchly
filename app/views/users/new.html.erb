<h1>Sign Up</h1> 

<%= form_for(@user, url:'/signup') do |f| %>
  
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>

  <%= f.submit "Submit" %>

<% end %>